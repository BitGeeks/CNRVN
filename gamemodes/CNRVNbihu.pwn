public OnPlayerEnterVehicle(playerid, vehicleid, ispassenger)
{
    // Admin Vehicle Lock System
    if(!ispassenger)
	{
		if(LockedCar[vehicleid] == 1 && PlayerCar[playerid] != vehicleid)
		{
		    // Anti Teleport System
			AntiCheatProtection[playerid] = 1;
			SetTimerEx("AntiTeleportTimer",4000,false,"i",playerid);
			// END
		    new Float:x, Float:y, Float:z;
	   		GetPlayerPos(playerid,x,y,z);
			SetPlayerPos(playerid,x,y,z+3);
		    SendClientMessage(playerid,RED,"PHUONG TIEN CUA ADMIN BAN KHONG THE DUNG!");
		}
	}
	// END
	
    GetPlayerLastVehicleID[playerid] = vehicleid;
	new string[150];
	if(!ispassenger)
	{
	    if(IsShopVehicle[vehicleid] != -1)
		{
		    if(strcmp(GetName(playerid), VehicleInfo[IsShopVehicle[vehicleid]][vOwner], false))
		    {
		        // Anti Teleport System
				AntiCheatProtection[playerid] = 1;
				SetTimerEx("AntiTeleportTimer",4000,false,"i",playerid);
				// END
		        format(string, sizeof(string), "Chiec xe nay la cua %s ban khong the su dung no.", VehicleInfo[IsShopVehicle[vehicleid]][vOwner]);
				new Float:x, Float:y, Float:z;
          		GetPlayerPos(playerid,x,y,z);
				SetPlayerPos(playerid,x,y,z+3);
		        SendClientMessage(playerid, RED, string);
		    }
		}
	}
	return 1;
}

