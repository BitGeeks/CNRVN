	    if(VehicleBombOwner[v] != playerid) continue;
	  	if(BomberStage{playerid} != 1) return SendClientMessage(playerid, RED, "Ban khong co qua bom dang gan tren bat cu phuong tien nao ca.");
	  	if(IsPlayerInAnyVehicle(playerid) && GetVehicleDriverID(v) == playerid || GetPlayerVehicleID(playerid) == v) return SendClientMessage(playerid, RED, "You cannot blow a vehicle up when your in it.");
		{
			new Float:x, Float:y, Float:z;
	       	GetVehiclePos(v, x, y, z);
			CreateExplosion(x, y, z, 7, 10);
 			CreateExplosion(x + 5.0, y, z, 7, 10);
 			CreateExplosion(x, y + 5.0, z, 7, 10);
 			CreateExplosion(x, y, z + 5.0, 7, 10);
	      	SetVehicleHealth(v, 0.0);
 			VehicleBombOwner[v] = INVALID_PLAYER_ID;
         	SendClientMessage(playerid, LIGHTGREEN, "Qua bom da no.");
         	BomberStage{playerid} = 0;
        	if(GetVehicleDriverID(v) != INVALID_PLAYER_ID)
          	{
            	new msg[150];
	      		SetPlayerHealth(GetVehicleDriverID(v), 0.0);
            	format(msg, sizeof(msg), "%s (%d) da chet trong vu no xe!", GetName(GetVehicleDriverID(v)), GetVehicleDriverID(v));
            	SendClientMessageToAll(RED, msg);
          		CallRemoteFunction("OnPlayerDeath", "ddd", GetVehicleDriverID(v), playerid, 40);
          	}
			return true;
		}
	}
	return true;
}

CMD:spec(playerid, params[])
{
    if(PlayerInfo[playerid][aLevel] < 1) return SendClientMessage(playerid,RED,"Khong tim thay lenh: De xem danh sach lenh day du, nhap /lenh.");
    new targetid, string[64];
    if(sscanf(params,"u", targetid)) return SendClientMessage(playerid, COLOR_ADMIN, "Usage: /spec [ten tai khoan/id]");
    if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi nay.Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(HasSpawned{targetid} == false) return SendClientMessage(playerid, RED, "Nguoi choi phai duoc spawn truoc khi ban co the theo doi.");
    if(targetid == playerid) return SendClientMessage(playerid, RED,"Ban khong the theo doi chinh minh.");
    if(IsOnduty{playerid} == 1)
	{
        IsOnduty{playerid} = 0;
		SetPlayerHealth(playerid, 100.0);
	}
    IsAdminSpectating{playerid} = true;
    IsPlayerBeingSpectated{targetid} = true;
    PlayerBeingSpectatedBy[targetid] = playerid;
	SpecInfo[playerid][IsSpectating] = targetid;
	TogglePlayerSpectating(playerid, true);
    if(IsPlayerInAnyVehicle(targetid))
    {
        SetPlayerInterior(playerid, GetPlayerInterior(targetid));
        SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(targetid));
        PlayerSpectateVehicle(playerid, GetPlayerVehicleID(targetid));
        format(fstr, sizeof(fstr), "{FFFFFF}Tong tai san cua %s (%d)", GetName(targetid), targetid);
        format(fstr2, sizeof(fstr2), "{FFFFFF}Tren nguoi: %s\nNgan hang: %s\nCredit: %s\nThu nhap: %s", formatInt(GetPlayerCash(targetid)), formatInt(BankInfo[targetid][Balance]), formatInt(BankInfo[targetid][Credit]), formatInt(GetPlayersBizEarnings(targetid)));
    	ShowPlayerDialog(playerid, WEALTH_DIALOG, DIALOG_STYLE_MSGBOX, fstr, fstr2, "Ok", "");
    	return 1;
    }
    else
    {
        SetPlayerInterior(playerid, GetPlayerInterior(targetid));
        SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(targetid));
        PlayerSpectatePlayer(playerid, targetid);
        format(fstr, sizeof(fstr), "{FFFFFF}Tong tai san cua %s (%d)", GetName(targetid), targetid);
        format(fstr2, sizeof(fstr2), "{FFFFFF}Tren nguoi: %s\nNgan hang: %s\nCredit: %s\nThu nhap: %s", formatInt(GetPlayerCash(targetid)), formatInt(BankInfo[targetid][Balance]), formatInt(BankInfo[targetid][Credit]), formatInt(GetPlayersBizEarnings(targetid)));
    	ShowPlayerDialog(playerid, WEALTH_DIALOG, DIALOG_STYLE_MSGBOX, fstr, fstr2, "Ok", "");
    }
    format(string, sizeof(string), "[SPEC]: Hien ban dang theo doi %s (%d)", GetName(targetid), targetid);
    SendClientMessage(playerid, COLOR_ADMIN, string);
    // Sending Message To Admins
	new astring[100];
	format(astring, sizeof(astring), "ADMIN: %s (%d) : Da su dung lenh /spec.", GetName(playerid), playerid);
   	// END
	return true;
}

