// SWAT Drag And Drop Commands
CMD:drag(playerid, params[])
{
	new string[250];
	new rangepass = 0, vehiclepass = 0, wantedpass = 0, dutypass = 0, eventpass = 0;
    if(GetTeam{playerid} != CLASS_SWAT) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
    if(HasDied{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban da chet.");
    if((gettime() - 120) < TimerInfo[playerid][SwatDragging_timer]) return SendClientMessage(playerid,RED,"Vui long cho truoc khi keo doi tuong khac.");
	if(HasBeenTied{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the keo doi tuong khi ban dang bi troi.");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid,RED,"Ban khong the keo doi tuong khi dang o tren xe, xuong xe truoc.");
	if(LastVehicle[playerid] == 0)
	{
	    SendClientMessage(playerid,RED,"Ban vao vao chiec xe truoc khi co the keo doi tuong.");
	    return 1;
	}
	foreach(Player, i)
	{
	    if(i == playerid) continue;
	    new Float:x, Float:y, Float:z;
    	GetPlayerPos(i, x, y, z);
    	new Float:hp;
    	GetPlayerHealth(i, hp);
    	if(hp > 0.0)
    	if(IsPlayerInRangeOfPoint(playerid, 9.0, x, y, z) && GetPlayerInterior(playerid) == GetPlayerInterior(i) && GetPlayerVirtualWorld(playerid) == GetPlayerVirtualWorld(i))
    	{
    	 	rangepass++;
			if(GetPlayerWantedLevel(i) >= 4)
			{
				wantedpass++;
				if(!IsPlayerInAnyVehicle(i))
				{
			    	vehiclepass++;
					if(IsOnduty{i} == 0)
					{
					    dutypass++;
						if(IsAtEvent{i} == 0)
						{
						    TimerInfo[playerid][SwatDragging_timer] = gettime();
                            eventpass++;
                            //
							format(string,sizeof(string),"Si quan S.W.A.T %s (%d) has placed you in his vehicle.",GetName(playerid),playerid);
							SendClientMessage(i,RED,string);

							SendClientMessage(playerid,GREEN,"Successfully Dragged To Your Vehicle");
							format(string,sizeof(string),"You have Dragged %s (%d) into the last vehicle you were in.",GetName(i),i);
							SendClientMessage(playerid,GREEN,string);
							SendClientMessage(playerid,-1,"{30ACAC}You Have Received {00FF22}$2,492 {30ACAC}As Bonus For Dragging Him To Your Vehicle.");

							format(string,sizeof(string),"{9900FF}Si quan S.W.A.T {00FF22}%s (%d) {FFFFFF}has placed the suspect {00FF22}%s (%d) {FFFFFF}in his vehicle.", GetName(playerid),playerid, GetName(i),i);
					  		SendClientMessageToAll(-1,string);

					  		GivePlayerCash(playerid, 2492);
					  		PlayerInfo[playerid][Drags] ++;

                            SetPlayerInterior(i,0);
		    				SetPlayerVirtualWorld(i,0);
							TogglePlayerControllable(i,0);
							PutPlayerInVehicle(i,LastVehicle[playerid],1);
							IsDetained[i] =1;
							PlayerInfo[i][TimesBeenDragged] += 1;
							//
							break;
						}
					}
				}
			}
		}
	}
	if(rangepass == 0)
	{
		SendClientMessage(playerid, RED, "There Are Currently No Players Close Enough that you can drag.");
	}
	else if(wantedpass == 0)
	{
		SendClientMessage(playerid, RED, "There Are Currently No Players Close Enough that you can drag.");
	}
	else if(vehiclepass == 0)
	{
		SendClientMessage(playerid,RED,"You cannot drag a suspect while they are in a vehicle, Get them to exit the vehicle first.");
	}
	else if(dutypass == 0)
	{
		SendClientMessage(playerid, RED, "You cannot drag on-duty Admins.");
	}
	else if(eventpass == 0)
	{
		SendClientMessage(playerid, RED, "You cannot drag Players While They Are In Event.");
	}
	return true;
}

