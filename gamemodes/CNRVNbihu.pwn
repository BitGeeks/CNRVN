// Coins Commands
CMD:duacoin(playerid,params[])
{
	new string[128];
	new coinid, amount;
    if(sscanf(params, "ui", coinid, amount))
	{
	    SendClientMessage(playerid,RED,"Usage: /duacoin [ten/id] [so luong coin]");
	    return 1;
	}
	if(IsPlayerNPC(coinid))
    {
	    SendClientMessage(playerid, RED, "Ban khong the dua coin cho [BOTS]");
	    return 1;
    }
	if(!IsPlayerConnected(coinid))
	{
		format(string,sizeof(string),"Nguoi choi ID (%d) dang khong ket noi toi may chu, vi vay ban khong the dua coin cho ho.",coinid);
        SendClientMessage(playerid,RED,string);
		return 1;
	}
	if(coinid == playerid)
	{
	    SendClientMessage(playerid,RED,"Ban khong the tu dua coin cho chinh minh, tai sao lai cu mat thoi gian?");
	    return 1;
	}
	if(PlayerInfo[playerid][Coins] < amount)
	{
	    SendClientMessage(playerid,RED,"Ban khong co du so coin do trong nguoi de dua cho bat cu ai.");
	    return 1;
	}
	if(amount > 100 || amount < 1)
	{
	    SendClientMessage(playerid,RED,"Xin hay nhap khoan coin tu 1 den 100.");
	    return 1;
	}
	format(fstr, sizeof(fstr),"Ban da dua %d. coin cho %s (%d)", amount, GetName(coinid), coinid);
	SendClientMessage(playerid, GREEN, fstr);
	
	format(fstr, sizeof(fstr),"%s (%d) da dua cho ban %d. Coin", GetName(playerid), playerid, amount);
	SendClientMessage(coinid, GREEN, fstr);
	
	PlayerInfo[coinid][Coins] += amount;
	PlayerInfo[playerid][Coins] -= amount;

	return 1;
}

