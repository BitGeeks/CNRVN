// Head Shot System
public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
	// On Duty Admin
	if(IsOnduty{playerid} == 1)
	{
	    return 1;
 	}
 	if(IsOnHiddenDuty{playerid} == 1)
	{
	    return 1;
 	}
	// END
	
    // Anti Team Attack
    
    // Law Officer Attacks On Law Officers
    if(GetTeam{issuerid} == CLASS_COPS || GetTeam{issuerid} == CLASS_CIA || GetTeam{issuerid} == CLASS_ARMY || GetTeam{issuerid} == CLASS_SWAT)
    {
    if(GetTeam{playerid} == CLASS_COPS || GetTeam{playerid} == CLASS_CIA || GetTeam{playerid} == CLASS_ARMY || GetTeam{playerid} == CLASS_SWAT)
    {
    	new Float:HP;
    	GetPlayerHealth(playerid, HP);
        if(GetPlayerWeapon(issuerid)) SetPlayerHealth(playerid, HP);
        // Armour System
        new Float:ARMOUR;
    	GetPlayerArmour(playerid, ARMOUR);
        if(GetPlayerWeapon(issuerid)) SetPlayerArmour(playerid, ARMOUR);
        // END
        SetPlayerHealth(playerid, 100.0);
        }
    }
    // END
    
    // Civilian Attacks On Law Officers
    if(GetTeam{issuerid} == CLASS_CIV)
    {
    if(GetTeam{playerid} == CLASS_COPS || GetTeam{playerid} == CLASS_CIA || GetTeam{playerid} == CLASS_ARMY || GetTeam{playerid} == CLASS_SWAT)
    {
    if(InDMS[issuerid] == 1)
	{
	    return 1;
 	}
    if((gettime() - 30) < TimerInfo[issuerid][Attack_timer]) return SetPlayerWantedLevel(issuerid, GetPlayerWantedLevel(issuerid) + 3);
    
    	SendClientMessage(issuerid, RED, "Pham toi: Tan cong - Cap do truy na 3 - Da phat lenh.");
    	SetPlayerWantedLevel(issuerid, GetPlayerWantedLevel(issuerid) + 3);
	    TimerInfo[issuerid][Attack_timer] = gettime();
	    PlayerInfo[issuerid][TotalCrimesCommited] += 1;
    	new current_zone;
     	current_zone = Player_Zone[issuerid];
		format(fstr, sizeof(fstr), "{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Tan cong, Vi tri: %s.", GetName(issuerid), issuerid, Zones[current_zone][zone_name]);
		CopRadio(BLUE, fstr);
	    }
    }
    // END
    
    // Law Officer Attacks On Civilians
    if(GetTeam{issuerid} == CLASS_COPS || GetTeam{issuerid} == CLASS_CIA || GetTeam{issuerid} == CLASS_ARMY || GetTeam{issuerid} == CLASS_SWAT)
    {
    if(GetTeam{playerid} == CLASS_CIV)
    {
    	if(GetPlayerWantedLevel(playerid) < 4)
		{
			new usemetext[250];
  			format(usemetext, sizeof(usemetext), "~r~KHONG DUOC TAN CONG NGUOI VO TOI");
		   	TextDrawSetString(Textdrawuseme0[issuerid], usemetext);
		    TextDrawShowForPlayer(issuerid, Textdrawuseme0[issuerid]);
		    SetTimerEx("EndAntiSpawnKill", 9000, false, "i", issuerid);
		    SendClientMessage(issuerid,RED,"Ban khong duoc phep tan cong nguoi vo toi, Vui long doc luat le /luatle!");
		    TogglePlayerControllable(issuerid,false);
		    SetTimerEx("AttackKillTimer", 6000, false, "i", issuerid);
		    }
 		}
    }
    // END

	// Death Stadium Head Shot System
    if(issuerid != INVALID_PLAYER_ID && weaponid == 34 && bodypart == 9)
    {
        if(InDMS[playerid] == 1)
        {
        // Showing Textdraw To Killer Player
        new headshot[250];
        format(headshot, sizeof(headshot), "~g~HEADSHOT!~n~~p~GIET BOI ~b~%s (%d)", GetName(playerid), playerid);
	    TextDrawSetString(Textdrawdmhskill0, headshot);
	    TextDrawShowForPlayer(issuerid, Textdrawdmhskill0);
	    SetTimerEx("EndAntiSpawnKill", 3000, false, "i", issuerid);
	    PlayerInfo[issuerid][dshsKills] ++;
	    // END

        // Showing Textdraw To Killed Player
        new headshot1[250];
        format(headshot1, sizeof(headshot1), "~r~HEADSHOT!~n~~p~GIET BOI ~b~%s (%d)", GetName(issuerid), issuerid);
	    TextDrawSetString(Textdrawdmhskilled0, headshot1);
	    TextDrawShowForPlayer(playerid, Textdrawdmhskilled0);
	    SetTimerEx("EndAntiSpawnKill", 3000, false, "i", playerid);
	    PlayerInfo[playerid][dshsDeaths] ++;
	    // END
        SetPlayerHealth(playerid, 0.0);
        }
    }
    // END
    return 1;
}
