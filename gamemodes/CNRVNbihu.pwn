    if((gettime() - 60) < TimerInfo[playerid][CMD_timer7]) return SendClientMessage(playerid, RED, "Vui long cho truoc khi su dung lenh nay lan nua.");//1 mins
    TimerInfo[playerid][CMD_timer7] = gettime();
