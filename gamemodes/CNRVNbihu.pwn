CMD:breakcuffs(playerid, params[])
{
    if(GetTeam{playerid} != CLASS_CIV) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
    if((gettime() - 5) < TimerInfo[playerid][CMD_timer5]) return SendClientMessage(playerid, RED, "Vui long cho truoc khi su dung lenh nay lan nua.");//1 mins
    TimerInfo[playerid][CMD_timer5] = gettime();
    if(HasBeenCuffed{playerid} == false) return SendClientMessage(playerid, RED, "Ban khong bi cong.");
    new BreakChances = random(5);
    switch(BreakChances)
    {
        case 0:
		{
			SendClientMessage(playerid, LIGHTGREEN, "Ban da pha thanh cong cai cong");
			TogglePlayerControllable(playerid, true);
			HasBeenCuffed{playerid} = false;
			SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
			SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 4);
		}
		case 1..4: return SendClientMessage(playerid, RED, "Ban da that bai trong viec pha cong.");
    }
    return true;
}

