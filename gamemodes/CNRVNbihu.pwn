	if(!IsPlayerConnected(invitee)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
	if(invitee == playerid) return SendClientMessage(playerid, RED, "Ban dang la truong nhom.");
