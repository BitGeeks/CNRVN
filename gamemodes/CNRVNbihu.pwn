    else if(GetPlayerWantedLevel(playerid) == 2)
    {
        SetPlayerWantedLevel(playerid, 1);
        SendClientMessage(playerid, GREEN, "Ban da mat mot cap do truy na ...");
        return 1;
    }
    else if(GetPlayerWantedLevel(playerid) == 1)
    {
        SetPlayerWantedLevel(playerid, 0);
        SendClientMessage(playerid, WHITE, "Ban hien da het bi truy na - vo toi");
    }
    return 1;
}
