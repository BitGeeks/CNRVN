CMD:raid(playerid, params[])
{
	if((gettime() - 60) < TimerInfo[playerid][CMD_timer36]) return SendClientMessage(playerid, RED, "Vui long cho truoc khi su dung lenh nay lan nua.");
    TimerInfo[playerid][CMD_timer36] = gettime();
	if(GetTeam{playerid} == CLASS_CIV || GetTeam{playerid} == CLASS_MEDIC) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
    if(GetPVarInt(playerid, "IsInHouse") == 1) return SendClientMessage(playerid, RED, "Ban phai o gan canh cua nha de su dung lenh nay.");
    if(GetPVarInt(playerid, "LastHouseCP") != -1)
    {
        if(strcmp(GetHouseOwner(GetPVarInt(playerid, "LastHouseCP")), "Nobody", false))
        {
            SetPVarInt(playerid, "IsInHouse", 1);
			SetPlayerHouseInterior(playerid, GetPVarInt(playerid, "LastHouseCP"));
			if(IsPlayerConnected(GetID(GetHouseOwner(GetPVarInt(playerid, "LastHouseCP")))))
			{
				new string[150], current_zone;
        		current_zone = Player_Zone[playerid];
				format(string, sizeof(string), "Nha cua ban da bi kham xet boi nhan vien thi hanh phap luat %s", Zones[current_zone][zone_name]);
				SendClientMessage(GetID(GetHouseOwner(GetPVarInt(playerid, "LastHouseCP"))), RED, string);
			}
			return true;
		}
		else
		{
	    	SendClientMessage(playerid, RED, "Ngoi nha nay khong thuoc so huu cua ai het.");
		}
    	return true;
	}
	else
	{
	    SendClientMessage(playerid, RED, "Ban khong o cua nha hop le.");
	}
	return true;
}

