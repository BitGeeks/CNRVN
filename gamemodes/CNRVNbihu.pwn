// Race Checkpoints
public OnPlayerEnterRaceCheckpoint(playerid)
{
    // Bank Robbery System
    if(bankrobbery[playerid] == 1)
	{
	    new string[500], pName[24];
		new loot = random(49000) + 2000;
		IsRobbingBank1{playerid} = false;
		IsRobbingBank2{playerid} = false;
		IsRobbingBank3{playerid} = false;
		IsRobbingBank4{playerid} = false;
		IsRobbingBank5{playerid} = false;
		IsRobbingBank6{playerid} = false;
		PlayerInfo[playerid][pHoldups]++;
		PlayerInfo[playerid][pCrimes]++;
		format(string, sizeof(string), "{30ACAC}Ban da cuop thanh cong {00FF22}%s {30ACAC}tu Cuop ngan hang.", formatInt(loot));
    	SendClientMessage(playerid, -1, string);
		GetPlayerName(playerid,(pName),sizeof(pName));
		GivePlayerCash(playerid,loot);
		format(string, sizeof(string), "{00FF22}%s (%d) {FFFFFF}da cuop thanh cong {00FF22}%s {FFFFFF}tu {00C7FF}vu cuop ngan hang",pName, playerid, formatInt(loot));
		SendClientMessageToAll(-1,string);
 		SetPlayerScore(playerid, GetPlayerScore(playerid)+1);
 		PlayerInfo[playerid][TimesBankRobbed] += 1;
 		PlayerInfo[playerid][TimesBankRobbedCash] += loot;
 		// Active Textdraw Box System
   		ActiveBox[playerid]=1;
	    // END
 		// Showing Textdraw
  		new usemetext[250];
    	format(usemetext, sizeof(usemetext), "CUOP NGAN HANG~n~THANH CONG!~n~~y~CUOP DUOC: ~g~$%d", loot);
	    TextDrawSetString(Textdrawuseme0[playerid], usemetext);
	    TextDrawShowForPlayer(playerid, Textdrawuseme0[playerid]);
