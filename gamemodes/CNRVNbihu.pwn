// Slap Command
CMD:slap(playerid, params[])
{
	new id;
	if(PlayerInfo[playerid][aLevel] < 3) return SendClientMessage(playerid,RED,"Khong tim thay lenh: De xem danh sach lenh day du, nhap /lenh.");
	if(sscanf(params, "u", id)) return SendClientMessage(playerid, COLOR_ADMIN, "Usage: /slap [playerid]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(IsLoggedIn{id} == 0) return SendClientMessage(playerid, RED, "Nguoi choi do khong dang nhap vao.");
	{
	    // Anti Teleport System
		AntiCheatProtection[id] = 1;
		SetTimerEx("AntiTeleportTimer",4000,false,"i",id);
		// END
	    new Float:Health;
		new Float:x, Float:y, Float:z;
		GetPlayerHealth(id,Health);
		SetPlayerHealth(id,Health-25);
		GetPlayerPos(id,x,y,z);
		SetPlayerPos(id,x,y,z+5);
	    format(fstr, sizeof(fstr), "Ban da slap %s (%d).", GetName(id), id);
	    SendClientMessage(playerid, COLOR_ADMIN, fstr);
	    // Showing Textdraws
	    GameTextForPlayer(id,"~r~ADMIN SLAP",4000,3);
		// END
		SendClientMessage(id,COLOR_ADMIN,"**ADMIN SERVER: da slap ban.");
		// Sending Message To Admins
        new astring[100];
        format(astring, sizeof(astring), "ADMIN: %s (%d) : Da su dung lenh /slap.", GetName(playerid), playerid);
        // END
    }
	return true;
}
