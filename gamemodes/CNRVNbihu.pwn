CMD:sellbizto(playerid, params[])
{
    new playerid2, price, string[150], string2[150], Float:posX, Float:posY, Float:posZ;
	if(sscanf(params, "ud", playerid2, price)) return SendClientMessage(playerid, RED, "Usage: /sellbizto [nick/id] [gia ca]");
	GetPlayerPos(playerid2, posX, posY, posZ);
	if(!IsPlayerConnected(playerid2)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
	if(!IsPlayerInRangeOfPoint(playerid, 10.0, posX, posY, posZ)) return SendClientMessage(playerid, RED, "Ban can phai o gan nguoi ban de co the mua biz tu ho.");
	for(new i = 0; i < MAX_BUSINESSES; i++)
	{
	    if(!IsPlayerInRangeOfPoint(playerid, 5.0, BizInfo[i][bExtX], BizInfo[i][bExtY], BizInfo[i][bExtZ]) && strcmp(BizInfo[i][bOwner], GetName(playerid), false)) return SendClientMessage(playerid, RED, "You must be outside near the door to sell a business you own to a player.");
	    {
			offer_price2[playerid2] = price;
			offerer_id2[playerid2] = playerid;
			sellingbizid[playerid] = i;
			format(string, sizeof(string), "De nghi mua biz toi %s (%d) voi gia %s, vui long cho phan hoi.", GetName(playerid2), playerid2, formatInt(price));
			SendClientMessage(playerid, 0x20B2AAAA, string);
			format(string2, sizeof(string2), "%s (%d) da de nghi %s cho ban voi gia %s, dung /chapnhanbiz /tuchoibiz", GetName(playerid), playerid, BizInfo[i][bName], formatInt(price));
			SendClientMessage(playerid2, 0x20B2AAAA, string2);
			SetTimerEx("AcceptCMDExpire", 40000, false, "d", playerid2);
		}
	}
	return true;
}

