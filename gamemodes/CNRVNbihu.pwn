// Making A Player To Cops Team
CMD:setcop(playerid, params[])
{
    new setid, skinid;
    if(PlayerInfo[playerid][aLevel] < 3) return SendClientMessage(playerid,RED,"Khong tim thay lenh: De xem danh sach lenh day du, nhap /lenh.");
	if(sscanf(params, "ud", setid, skinid)) return SendClientMessage(playerid, COLOR_ADMIN, "Usage: /setcop [ten tai khoan/id] [Skin-ID]");
	if(!IsPlayerConnected(setid)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(IsLoggedIn{setid} == 0) return SendClientMessage(playerid, RED, "Nguoi choi do khong dang nhap vao.");
	if(skinid < 0 || skinid > 299) return SendClientMessage(playerid, RED, "ID ngoai hinh khong hop le.");
    {
	    format(fstr, sizeof(fstr), "Ban da dua %s (%d) To Cop With Skin ID To %d.", GetName(setid), setid, skinid);
	    SendClientMessage(playerid, COLOR_ADMIN, fstr);
	    
        SendClientMessage(setid,GREEN,"**Admin da dua ban lam canh sat.");
	    
	    SetPlayerSkin(setid, skinid);
	    // Saving Player Skin
	   	new file[128], pname[MAX_PLAYER_NAME];
		GetPlayerName(setid, pname, sizeof(pname));
		format(file, sizeof(file), "CNRVN/Skins/%s.ini", pname);
		dini_Create(file);
		dini_IntSet(file, "skin", GetPlayerSkin(setid));
		// END
	    GetTeam{setid} = CLASS_COPS;
	    // Sending Message To Admins
	    new astring[100];
	    format(astring, sizeof(astring), "ADMIN: %s (%d) : Da su dung lenh /setcop.", GetName(playerid), playerid);
	    // END
	    }
		return true;
}
