// Nitro Kit System
public NitroReset()
{
	for(new i = 0; i<MAX_PLAYERS; i++)
	{
	    if(!IsPlayerInInvalidNossVehicle(i,GetPlayerVehicleID(i)))
		{
		    if(PlayerInfo[i][NitroKitLevel] > 0)
    		{
    		    new vehicle = GetPlayerVehicleID(i);
		    	AddVehicleComponent(vehicle, 1010);
	    	}
  		}
	}
	return 1;
}

IsPlayerInInvalidNossVehicle(playerid,vehicleid)
{
    #define MAX_INVALID_NOSS_VEHICLES 29

    new InvalidNosVehicles[MAX_INVALID_NOSS_VEHICLES] =
    {
	581,523,462,521,463,522,461,448,468,586,
	509,481,510,472,473,493,595,484,430,453,
	452,446,454,590,569,537,538,570,449
	};
    vehicleid = GetPlayerVehicleID(playerid);

    if(IsPlayerInVehicle(playerid,vehicleid))
    {
		for(new i = 0; i < MAX_INVALID_NOSS_VEHICLES; i++)
		{
		    if(GetVehicleModel(vehicleid) == InvalidNosVehicles[i])
		    {
		        return true;
		    }
		}
    }
    return false;
}
