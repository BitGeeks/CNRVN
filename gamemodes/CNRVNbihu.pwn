public SendPlayerToJail(playerid)
{
	new cell = random(6);
	if(cell == 0)
	{
	    new time;
	    if(HasPlayerDiedInJail{playerid} == true)
	    {
	        HasPlayerDiedInJail{playerid} = false;
	        time = JailTime[playerid];
		}
		else
		{
			time = random(30) + 30;//Random 2-3 mins
		}
		// Anti Teleport System
		AntiCheatProtection[playerid] = 1;
		SetTimerEx("AntiTeleportTimer",4000,false,"i",playerid);
		// END
	    new inmateid = random(100000) + random(123456);
    	InMateID[playerid] = inmateid;
    	IsJailed{playerid} = true;
    	IsCaged{playerid} = false;
		HasBeenCuffed{playerid} = false;
    	IsInCell{playerid} = 1;//
    	GoToJail{playerid} = false;
    	JailTime[playerid] = time;
    	SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);
    	SetPlayerHealth(playerid, 100.0);
    	SetPlayerColor(playerid, WHITE);
    	SetPlayerWantedLevel(playerid, 0);
    	ResetPlayerWeapons(playerid);
    	SetPlayerInterior(playerid, 3);
		SetPlayerPos(playerid, 193.5904,175.7956,1003.0234);
		SetPlayerFacingAngle(playerid,357.7934);
		SetCameraBehindPlayer(playerid);
    	TogglePlayerControllable(playerid, true);
    	HasBeenArrested{playerid} = false;
    	new string[150];
    	format(string, sizeof(string), "Ban da bi giam %d giay.", time);
    	SendClientMessage(playerid, GREEN, string);
