CMD:hit(playerid, params[])
{
    new amount;
    if(IsJailed{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi dang bi giam giu.");
    if(sscanf(params, "ud", ID, amount)) return SendClientMessage(playerid, RED, "Usage: /hit [Nick-ID] [so tien]");
	if(amount < 10000) return SendClientMessage(playerid, RED, "Hitman khong lam viec voi so tien it hon $10,000.");
	if(!IsPlayerConnected(ID)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
	if(IsPlayerNPC(ID)) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay voi [BOTS] may chu");
	if(ID == playerid) return SendClientMessage(playerid, RED, "Ban khong the dat hop dong chinh ban.");
	if(GetPlayerCash(playerid) < amount) return SendClientMessage(playerid, RED, "Ban khong du hien de dat hop dong.");
	if(HasHit[ID] > 0) return SendClientMessage(playerid, RED, "Nguoi choi do dang bi nguoi khac dat hop dong giet.");
	{
	    // Showing Textdraws
  		new usemetext[250];
    	format(usemetext, sizeof(usemetext), "~w~Hop dong sat thu ~n~~w~%s (%d) voi %s", GetName(ID), ID, formatInt(amount));
	    TextDrawSetString(Textdrawuseme0[playerid], usemetext);
	    TextDrawShowForPlayer(playerid, Textdrawuseme0[playerid]);
	    SetTimerEx("EndAntiSpawnKill", 9000, false, "i", playerid);
	    // END
	    PlayerInfo[playerid][TimesHitPlaced] += 1;
	    HasHit[ID] = amount;
	    HasPlacedHit[ID] = playerid;
	    GivePlayerCash(playerid, -amount);
		format(fstr, sizeof(fstr), "Ban da dat hop dong sat thu de giet %s (%d) voi %s.", GetName(ID), ID, formatInt(amount));
		SendClientMessage(playerid, 0x20B2AAAA, fstr);
		format(fstr2, sizeof(fstr2), "{00FF22}%s (%d) {FFFFFF}da dat {00FF22}%s {F81414}cho hop dong sat thu giet {FFFFFF}On %s (%d).", GetName(playerid), playerid, formatInt(amount), GetName(ID), ID);
		SendClientMessageToAll(-1, fstr2);
		format(fstr, sizeof(fstr), "%s (%d) da dat mot hop dong sat thu giet ban voi %s.", GetName(playerid), playerid, formatInt(amount));
		SendClientMessage(ID, 0x20B2AAAA, fstr);
	}
    return true;
}

CMD:cancelhit(playerid, params[])
{
	new contract_id;
    if(sscanf(params, "u", contract_id)) return SendClientMessage(playerid, RED, "Usage: /cancelhit [ten tai khoan/id]");
	if(!IsPlayerConnected(contract_id)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
	if(contract_id == playerid) return SendClientMessage(playerid, RED, "Ban khong the huy bo hop dong cho chinh ban.");
	if(HasHit[contract_id] > 0 && HasPlacedHit[contract_id] == playerid)
	{
	    GivePlayerCash(playerid, HasHit[playerid]);
	    HasHit[contract_id] = 0;
	    HasPlacedHit[contract_id] = -1;
	    format(fstr, sizeof(fstr), "Ban da huy bo hop dong cho %s (%d).", GetName(contract_id), contract_id);
	    SendClientMessage(playerid, 0x20B2AAAA, fstr);
		format(fstr, sizeof(fstr), "{00FF22}%s (%d) {FFFFFF}da {F81414}huy bo {FFFFFF}hop dong giet {00FF22}%s (%d)", GetName(playerid), playerid, GetName(contract_id), contract_id);
   		SendClientMessageToAll(-1, fstr);
	}
	else
	{
	    SendClientMessage(playerid, RED, "Ban chi co the huy bo hop dong sat thu dat ra boi chinh ban.");
	}
	return 1;
}

CMD:hits(playerid, params[])
{
    if(IsJailed{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi dang bi giam giu.");
    new count = 0;
    foreach(Player, i)
    {
	    if(HasHit[i] > 0)
	    {
		    format(fstr, sizeof(fstr), "%s (%d) %s", GetName(i), i, formatInt(HasHit[i]));
		    SendClientMessage(playerid, GREEN, fstr);
		    count++;
		}
