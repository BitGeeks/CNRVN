    if(!ProxDetectorS(5.0, playerid, healee)) return SendClientMessage(playerid, RED, "Nguoi choi do khong du gan.");
    if(IsPlayerInAnyVehicle(healee) && IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "Ban khong the hoi suc khoe cho nguoi choi dang tren xe.");
    {
        format(fstr, sizeof(fstr), "Ban da de nghi hoi suc khoe cho %s (%d) voi gia $%d, vui long cho ho tra loi.", GetName(healee), healee, price);
        SendClientMessage(playerid, YELLOW, fstr);
