CMD:reply(playerid, params[])
{
	new msg[80];
	if(chattingwith[playerid] == -1) return SendClientMessage(playerid, RED, "Ban khong co tin nhan rieng tu nao de doc.");
    if(IsMuted{playerid} == 1) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban dang bi khoa chat.");
    if(sscanf(params, "s[64]", msg)) return SendClientMessage(playerid, RED, "Usage: /reply [tin nhan]");
    if(IsIgnoringPMs{chattingwith[playerid]} == 1) return SendClientMessage(playerid, RED, "Nguoi choi do tu choi nhan tin nhan ca nhan trong luc nay.");
    if(!IsPlayerConnected(chattingwith[playerid])) return SendClientMessage(playerid, RED, "Nguoi choi da bi mat ket noi.");
    if(strlen(msg) < 1 || strlen(msg) > 64) return SendClientMessage(playerid, RED, "Tin nhan cua ban chi co the tu 1-80 ky tu.");
    {
        format(fstr, sizeof(fstr),"PM Sent To %s (%d): %s", GetName(chattingwith[playerid]), chattingwith[playerid], msg);
        SendClientMessage(playerid, YELLOW, fstr);
        format(fstr, sizeof(fstr),"PM From %s (%d): %s", GetName(playerid), playerid, msg);
        SendClientMessage(chattingwith[playerid], YELLOW, fstr);
        SendClientMessage(chattingwith[playerid], 0x20B2AAAA, "Dung /r hoac /reply de tra loi tin nhan nhanh");
        PlayerPlaySound(chattingwith[playerid],1085,0.0,0.0,0.0);
        chattingwith[chattingwith[playerid]] = playerid;
	}
	return true;
}

CMD:pm(playerid, params[])
{
	new id, msg[100];
    if(IsMuted{playerid} == 1) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban dang bi khoa chat.");
    if(sscanf(params, "us[64]", id, msg)) return SendClientMessage(playerid, RED, "Usage: /pm [ten tai khoan/id] [msg]");
    if(!IsPlayerConnected(id)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(strlen(msg) < 1 || strlen(msg) > 64) return SendClientMessage(playerid, RED, "Tin nhan cua ban chi co the tu 1-100 ky tu.");
    if(playerid == id) return SendClientMessage(playerid, RED, "Ban khong the tu PM cho chinh minh.");
    if(IsPlayerNPC(id)) return SendClientMessage(playerid, RED, "Ban khong the /pm cho [BOTS] may chu");
    if(IsIgnoringPMs{id} == 1) return SendClientMessage(playerid, RED, "Nguoi choi do tu choi nhan tin nhan ca nhan trong luc nay.");
    {
        format(fstr, sizeof(fstr), "PM gui toi %s (%d): %s", GetName(id), id, msg);
        SendClientMessage(playerid, YELLOW, fstr);
        format(fstr, sizeof(fstr), "PM tu %s (%d): %s", GetName(playerid), playerid, msg);
        SendClientMessage(id, YELLOW, fstr);
        SendClientMessage(id, 0x20B2AAAA, "Dung /r hoac /reply de tra loi nhanh");
        PlayerPlaySound(id,1085,0.0,0.0,0.0);
        chattingwith[id] = playerid;
    }
	return true;
}

