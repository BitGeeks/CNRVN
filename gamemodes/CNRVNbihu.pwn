CMD:acceptbiz(playerid, params[])
{
	new Float:posX, Float:posY, Float:posZ;
	GetPlayerPos(offerer_id2[playerid], posX, posY, posZ);
    if(!IsPlayerConnected(offerer_id2[playerid])) return SendClientMessage(playerid, RED, "Nguoi ban da mat ket noi.");
    if(offerer_id2[playerid] == -1) return SendClientMessage(playerid, RED, "Ban khong duoc de nghi mua biz gan day.");
	if(GetPlayerCash(playerid) < offer_price2[playerid]) return SendClientMessage(playerid, RED, "Ban khong co du tien de hoan thanh tac vu nay.");
    if(!IsPlayerInRangeOfPoint(playerid, 10.0, posX, posY, posZ)) return SendClientMessage(playerid, RED, "Ban can phai o gan nguoi ban de co the mua biz tu ho.");
    new string[150], string2[150];
    new file[40];
	format(file, sizeof(file), BIZ_FILE, sellingbizid[playerid]);
 	format(bizlabel, sizeof(bizlabel), "Chu so huu\ncua biz: %s", GetName(playerid));
	Update3DTextLabelText(bLabel[BizID[playerid]], YELLOW, bizlabel);
	new INI:bFilee = INI_Open(file);
	INI_WriteString(bFilee, "bName", bizlabel);
	INI_WriteString(bFilee, "bOwner", GetName(playerid));
	INI_Close(bFilee);
	GivePlayerCash(offerer_id2[playerid], BizInfo[sellingbizid[playerid]][bPrice]/2);
	GivePlayerCash(playerid, - offer_price2[playerid]);
	GivePlayerCash(offerer_id2[playerid], offer_price2[playerid]);
	format(string, sizeof(string), "Da ban biz cho %s voi gia %s", GetName(playerid), formatInt(offer_price2[playerid]));
	SendClientMessage(offerer_id2[playerid], 0x20B2AAAA, string);
	format(string2, sizeof(string2), "Da mua biz tu %s voi gia %s", GetName(offerer_id2[playerid]), formatInt(offer_price2[playerid]));
	SendClientMessage(playerid, 0x20B2AAAA, string2);
	offer_price2[playerid] = -1;
	offerer_id2[playerid] = -1;
	sellingbizid[playerid] = -1;
	return true;
}

