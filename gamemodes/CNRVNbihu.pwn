// Countdown System
public Counts(amount, const interval)
{
    if(CountsTimer == -1)
	{
 		CountsTimer = SetTimer("pCounts", interval, true);
        TextDrawShowForAll(Textdrawcount0);
        CountsValue = amount + 1;
        pCounts();
        return true;
    }
    return false;
}

KillCounts()
{
	if(CountsTimer != -1)
	{
 		TextDrawHideForAll(Textdrawcount0);
        KillTimer(CountsTimer);
        CountsTimer = -1;
        TextDrawShowForAll(Textdrawcount1);
	    SetTimerEx("EndAntiSpawnKill", 5000, false, "i", CountsTimer);
        return true;
    }
    return false;
}

forward pCounts();
public pCounts()
{
	if(--CountsValue < 0)
	{
		KillCounts();
    }
	else
	{
        new string[32];
        valstr(string, CountsValue);
        TextDrawSetString(Textdrawcount0, string);
    }
    return 1;
}
