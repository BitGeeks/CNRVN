// Rape System
public checkchlamy()
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(chlamydia[i] == 1 && IsPlayerConnected(i))
		{
			new Float:health;
			GetPlayerHealth(i, health);
			SetPlayerHealth(i, health-5-random(10));
		}
	}
	return 1;
