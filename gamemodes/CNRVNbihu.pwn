public FailureToPayTicket(playerid)
{
	if(HasBeenIssuedTicket{playerid} == true && HasBeenArrested{playerid} == false)
	{
	    PlayerInfo[playerid][TotalCrimesCommited] += 1;
		new current_zone;
        current_zone = Player_Zone[playerid];
		SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 6);
		SendClientMessage(playerid, RED, "Pham toi: Khong tra tien nop phat ve phat - Cap do truy na 6 - Da phat lenh.");
		format(fstr, sizeof(fstr), "{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Khong nop phat, Vi tri: %s.", GetName(playerid), playerid, Zones[current_zone][zone_name]);
		CopRadio(BLUE, fstr);
		HasBeenIssuedTicket{playerid} = false;
		TicketIssuer[playerid] = -1;
	}
	return 1;
}

