	if(!IsPlayerConnected(healee)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(healee == playerid) return SendClientMessage(playerid, RED, "Ban khong the tu hoi suc khoe cho chinh ban.");
