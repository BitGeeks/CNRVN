CMD:givecash(playerid, params[])
{
	new cashid, amount;
    if(sscanf(params, "ui", cashid, amount))
	{
	    SendClientMessage(playerid,RED,"Usage: /duatien (id/ten) [so tien]");
	    return 1;
	}
	if(cashid == playerid)
	{
	    SendClientMessage(playerid,RED,"Ban khong the dua tien cho chinh ban. Tai sao phai ton thoi gian?");
	    return 1;
	}
	if(GetPlayerCash(playerid) < amount)
	{
	    SendClientMessage(playerid,RED,"Ban khong co du so tien do trong nguoi de dua no cho ai do.");
	    return 1;
	}
	if(amount < 500 || amount > 5000000)
	{
	    SendClientMessage(playerid,RED,"Hay nhap so tien tu $500 den $5,000,000.");
	    return 1;
	}
	if(!ProxDetectorS(8.0, playerid, cashid))
	{
	    SendClientMessage(playerid,RED,"Nguoi choi do khong o gan de ban co the dua tien.");
	    return 1;
	}
	    // User Stats System
	    PlayerInfo[playerid][TimesCashGiven] += 1;
	    PlayerInfo[playerid][TimesCashGivenCash] += amount;
	    PlayerInfo[cashid][TimesCashReceived] += 1;
	    PlayerInfo[cashid][TimesCashReceivedCash] += amount;
		// END
		GivePlayerCash(playerid, -amount);
		GivePlayerCash(cashid, amount);
		format(fstr, sizeof(fstr),"{30ACAC}Ban da gui {00FF22}%s {30ACAC}cho nguoi choi %s (%d).", formatInt(amount), GetName(cashid), cashid);
		SendClientMessage(playerid, -1, fstr);
		format(fstr, sizeof(fstr),"{30ACAC}%s (%d) da dua ban {00FF22}%s.", GetName(playerid), playerid, formatInt(amount));
		SendClientMessage(cashid, -1, fstr);
    	return true;
}

// Gascan Command
CMD:gascan(playerid, params[])
{
    new string[128], amount;
    if(sscanf(params, "i", amount))
	{
	    SendClientMessage(playerid,RED,"Usage: /gascan [So luong]");
	    return 1;
	}
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
	{
		SendClientMessage(playerid,RED,"Ban phai la nguoi lai xe de thuc hien lenh nay");
		return 1;
	}
	if(amount < 1 || amount > 100)
	{
		SendClientMessage(playerid,RED,"Nhap so luong tu 1 den 100.");
	    return 1;
	}
	if(PlayerInfo[playerid][Gascan] < amount)
	{
	    SendClientMessage(playerid,RED,"Binh gas cua ban khong du so nhien lieu do.");
	    return 1;
	}

	format(string, sizeof(string),"Ban da su dung %d nhien lieu tu binh gas cua ban de tiep nhien lieu cho ban.", amount);
	SendClientMessage(playerid, GREEN, string);

	new vid = GetPlayerVehicleID(playerid);
	GetVehicleFuel[vid] += amount;
	
	new objective;
	SetVehicleParamsEx(vid, VEHICLE_PARAMS_ON, lights, alarm, doors, bonnet, boot, objective);

	PlayerInfo[playerid][Gascan] -= amount;

    return true;
}
