CMD:sell(playerid, params[])
{
    if(GetTeam{playerid} != CLASS_CIV) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");

	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "Ban chi co the su dung lenh nay khi dang o tren phuong tien");


    if(IsPlayerInRangeOfPoint(playerid,1,2165.0088,1406.8373,10.8203) || IsPlayerInRangeOfPoint(playerid,1,1987.2689,2047.1064,10.8203)
	|| IsPlayerInRangeOfPoint(playerid,1,-1971.0582,303.3146,35.1719) || IsPlayerInRangeOfPoint(playerid,1,-1640.8607,1213.9027,7.0391)
	|| IsPlayerInRangeOfPoint(playerid,1,542.9143,-1275.7061,17.2422) || IsPlayerInRangeOfPoint(playerid,1,2119.5430,-1151.7925,24.0248))
	{
	
	new vehicleid = GetPlayerVehicleID(playerid);
	
	if(IsPlayerInOwnedVehicle(playerid, vehicleid) == 1) return SendClientMessage(playerid, RED, "Ban khong the ban xe Vincar cua ban.");
	
	if (vehicleid == BviVehicleLandstalker || vehicleid == BviVehicleSecuricar || vehicleid == BviVehicleSandking || vehicleid == BviVehicleMesa || vehicleid == BviVehicleElegant || vehicleid == BviVehicleTowtruck || vehicleid == BviVehicleBlade || vehicleid == BviVehiclePhoenix)
 	{
 	    if(HasBviStarted == 1)
		{
		    PlayerInfo[playerid][TimesVehicleSold] += 1;
	 	    // Destroying Vehicle
	 	    DestroyVehicle(GetPlayerVehicleID(playerid));
	 	    // END
	 	    format(fstr, sizeof(fstr), "{00FF22}%s (%d) {FFFFFF}da ban thanh cong {F81414}phuong tien bonus cua ngay hom nay {FFFFFF}voi gia {00FF22}$122,479", GetName(playerid), playerid);
	      	SendClientMessageToAll(-1, fstr);
	      	SendClientMessage(playerid,0x20B2AAAA,"Ban da ban thanh cong phuong tien bonus cua ngay hom nay voi gia {00FF22}$122,479");
		    GivePlayerCash(playerid, 122479);
		    // Closing Bonus Vehicle
			DestroyVehicle(BviVehicleLandstalker);
			DestroyVehicle(BviVehicleSecuricar);
			DestroyVehicle(BviVehicleSandking);
			DestroyVehicle(BviVehicleMesa);
			DestroyVehicle(BviVehicleElegant);
			DestroyVehicle(BviVehicleTowtruck);
			DestroyVehicle(BviVehicleBlade);
			DestroyVehicle(BviVehiclePhoenix);
			//
	  		BviLandstalker = 0;
	    	BviSecuricar = 0;
		    BviSandking = 0;
		    BviMesa = 0;
		    BviElegant = 0;
		    BviTowtruck = 0;
		    BviBlade = 0;
		    BviPhoenix = 0;
		    // END
		    HasBviStarted = 0;
	    }
	    else
	    {
	    	SendClientMessage(playerid,RED,"Ban khong the ban phuong tien nay");
    	}
	}
	else
	{
	    SendClientMessage(playerid,RED,"Ban khong the ban phuong tien nay");
	    }
 	}
 	else
 	{
 	    SendClientMessage(playerid, RED, "Ban phai o cua hang xe de ban phuong tien nay");
  	}
	return 1;
}
