// Lotto Command
CMD:lotto(playerid,params[])
{
	new ru[250],ti;
	if(IsLottoStarted==false)return SendClientMessage(playerid, RED, "Thoi gian mua loto da ket thuc.");
	if(sscanf(params,"i",ti))return SendClientMessage(playerid,RED,"Usage: /lotto [1-50]");
	format(ru,sizeof(ru),"Correct Usage:/lotto 0-%i",numrange);
	if(ti > numrange || ti < 0)return SendClientMessage(playerid,RED,ru);
	format(ru,sizeof(ru),"You need to have %s to buy ticket.",formatInt(ticketprice));
	if(GetPlayerCash(playerid)<ticketprice)return SendClientMessage(playerid,RED,ru);
	if(takenticket[playerid]==1)return SendClientMessage(playerid,RED,"Ban da mua mot ve xo so.");
	if(IsNumTaken(ti))return SendClientMessage(playerid,RED,"Xin loi nhung so ve xo so nay da duoc nguoi khac mua truoc.");
	takenticket[playerid]=1;
	GivePlayerCash(playerid,-ticketprice);
	ticketnum[playerid]=ti;
	// Showing Textdraw
	HideDraw(playerid);
	new usemetext[250];
 	format(usemetext, sizeof(usemetext), "~p~SO LOTO ~y~%d~n~~w~JACKPOT HIEN TAI ~g~%s",ti,formatInt(lottoadd));
	GameTextForPlayer(playerid, usemetext ,10000,3);
 	// END
	format(ru,sizeof(ru),"So xo so hien tai cua ban: %i, Jackpot hien tai: %s. Loto se duoc xo vao luc 18:00.",ti,formatInt(lottoadd));
	SendClientMessage(playerid,0x20B2AAAA,ru);
	return 1;
}
