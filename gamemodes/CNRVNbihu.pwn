public OnPlayerRequestSpawn(playerid)
{
    if(IsPlayerNPC(playerid)) return 1;

    if(IsLoggedIn{playerid} == 0)
	{
		SendClientMessage(playerid, RED, "Ban PHAI dang ky va dang nhap truoc khi co the spawn.") && Kick(playerid);
		return 0;
	}
    else if(GetTeam{playerid} == CLASS_COPS && PlayerInfo[playerid][pWanted] >= 1 || GetPlayerWantedLevel(playerid) >= 1)
    {
		SendClientMessage(playerid, RED, "Ban khong the spawn thanh nhan vien thi hanh phap luat, khi dang bi truy na.");
      	ForceClassSelection(playerid);
      	return 0;
    }
    else if(GetTeam{playerid} == CLASS_ARMY)
    {
        if(PlayerInfo[playerid][pWanted] < 1 || GetPlayerWantedLevel(playerid) < 1)
        {
			if(PlayerInfo[playerid][Arrests] >= 200)
    		{
				return 1;
			}
    		else
    		{
				SendClientMessage(playerid, RED, "Ban can 200 lan bat de tro thanh linh SAAS.");
      			ForceClassSelection(playerid);
      			return 0;
    		}
    	}
    	else
    	{
			SendClientMessage(playerid, RED, "Ban khong the spawn thanh nhan vien thi hanh phap luat, khi dang bi truy na.");
      		ForceClassSelection(playerid);
    	}
      	return 0;
    }
    else if(GetTeam{playerid} == CLASS_SWAT)
    {
        if(PlayerInfo[playerid][pWanted] < 1 || GetPlayerWantedLevel(playerid) < 1)
        {
			if(PlayerInfo[playerid][sLevel] > 0)
			{
			    return 1;
   			}
			if(PlayerInfo[playerid][scLevel] > 0)
    		{
				return 1;
    		}
    		else
    		{
				SendClientMessage(playerid, RED, "Ban khong phai la thanh vien cua S.W.A.T, ban can phai nop don lam S.W.A.T len dien dan.");
      			ForceClassSelection(playerid);
      			return 0;
    		}
    	}
    	else
    	{
			SendClientMessage(playerid, RED, "Ban khong the spawn thanh nhan vien thi hanh phap luat, khi dang bi truy na.");
      		ForceClassSelection(playerid);
    	}
      	return 0;
    }
    else if(GetTeam{playerid} == CLASS_MEDIC)
    {
        if(PlayerInfo[playerid][pWanted] > 0 || GetPlayerWantedLevel(playerid) > 0)
        {
			SendClientMessage(playerid, RED, "Ban khong the spawn thanh bac si, khi dang bi truy na.");
      		ForceClassSelection(playerid);
      		return 0;
    	}
    	else
    	{
    	return 1;
    	}
    }
    return 1;
}

