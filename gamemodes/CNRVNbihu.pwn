public AutoUncage(playerid)
{
	if(IsCaged{playerid} == true)
	{
		if(CageTime[playerid] > 0)
		{
		    HideDraw(playerid);
		    CageTime[playerid] --;
			new ctime = (CageTime[playerid]);
		    new usemetext[250];
	    	format(usemetext, sizeof(usemetext), "~r~Giam giu ~y~Thoi gian con lai: ~g~%d ~w~giay", ctime);
		    TextDrawSetString(Textdrawuseme0[playerid], usemetext);
		    TextDrawShowForPlayer(playerid, Textdrawuseme0[playerid]);
		    SetTimerEx("EndAntiSpawnKill", 5000, false, "i", playerid);
		    // END
         	return 1;
      	}
		if(JailTime[playerid] == 0)
     	{
     	    KillTimer(CageTimer[playerid]);
			CageTime[playerid] = 0;
			PlayerInfo[playerid][cTime] = 0;
			IsCaged{playerid} = false;
			if(IsPlayerConnected(playerid) && IsLoggedIn{playerid} == 1)
			{
			    SaveUserStats(playerid);
			}
			// Anti Teleport System
			AntiCheatProtection[playerid] = 1;
			SetTimerEx("AntiTeleportTimer",4000,false,"i",playerid);
			// END
			SetPlayerWantedLevel(playerid, 0);
			SetPlayerInterior(playerid, 0);
			SetPlayerVirtualWorld(playerid,0);
			SetPlayerPos(playerid, 2286.9150,2428.6892,10.8203);
			SetPlayerFacingAngle(playerid, 175.3000);
			SetCameraBehindPlayer(playerid);
			format(fstr, sizeof(fstr), "{26FF4E}%s (%d) {FFFFFF}da duoc tha tu {FF0000}nha tu.", GetName(playerid), playerid);
        	SendClientMessageToAll(COLOR_ADMIN, fstr);
	  	}
	  	return 1;
	}
    return 1;
}

