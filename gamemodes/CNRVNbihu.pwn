       	TogglePlayerControllable(targetid, true);
	}
   	return true;
}

CMD:rob(playerid, params[])
{
	new amount;
	new rangepass = 0, vehiclepass = 0, dutypass = 0, hdutypass = 0, eventpass = 0;
    if(GetTeam{playerid} != CLASS_CIV) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
    if(HasDied{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban da chet.");
    if(HasSpawned{playerid} == false) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban da chet.");
    if((gettime() - 70) < TimerInfo[playerid][CMD_timer37]) return SendClientMessage(playerid, RED, "Vui long cho truoc khi ban co the cuop lan nua.");
    if(InDMS[playerid] == 1) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi ban dang o trong death stadium!");
    if(IsJailed{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the su dung lenh nay khi dang bi giam giu.");
	if(HasBeenTied{playerid} == true) return SendClientMessage(playerid, RED, "Ban khong the cuop nguoi choi khi ban dang bi troi.");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "Ban khong the cuop nguoi choi khi dang o tren xe.");
	if(GetPlayerInterior(playerid) >= 1) return SendClientMessage(playerid, RED, "Ban khong the cuop nguoi choi khi dang o trong noi that.");
	foreach(Player, i)
	{
	    if(i == playerid) continue;
	    new Float:x, Float:y, Float:z;
    	GetPlayerPos(i, x, y, z);
    	new Float:hp;
    	GetPlayerHealth(i, hp);
    	if(hp > 0.0)
    	if(IsPlayerInRangeOfPoint(playerid, 8.0, x, y, z) && GetPlayerInterior(playerid) == GetPlayerInterior(i) && GetPlayerVirtualWorld(playerid) == GetPlayerVirtualWorld(i))
    	{
			rangepass++;
			if(!IsPlayerInAnyVehicle(i))
			{
  				vehiclepass++;
				if(IsOnduty{i} == 0)
				{
    				dutypass++;
				if(IsOnHiddenDuty{i} == 0)
				{
				    hdutypass++;
					if(IsAtEvent{i} == 0)
					{
    					eventpass++;
					    if(GetTeam{i} == CLASS_COPS || GetTeam{i} == CLASS_CIA || GetTeam{i} == CLASS_SWAT || GetTeam{i} == CLASS_ARMY)
						{
		    			if (GetPlayerCash(i) > 100)
						{
						    PlayerInfo[playerid][TotalCrimesCommited] += 1;
						    amount = GetPlayerCash(i)/50;
     						//
     						GivePlayerCash(i, - amount);
							GivePlayerCash(playerid, amount);
							PlayerInfo[playerid][TimesPlayerRobbed] += 1;
						    PlayerInfo[playerid][TimesPlayerRobbedCash] += amount;
						    //
						    new string[150], string2[150], string3[150];
						    TimerInfo[playerid][CMD_timer37] = gettime();
						    PlayerInfo[playerid][pCrimes] ++;
							SetPlayerScore(playerid, GetPlayerScore(playerid)+1);
							SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 6);
							new current_zone;
       						current_zone = Player_Zone[playerid];
					        SendClientMessage(playerid, RED, "Pham toi: Cuop cua si quan thi hanh phap luat - Cap do truy na 6 - Da phat lenh.");
							format(string, sizeof(string), "Ban da cuop %s tu %s (%d).", formatInt(amount), GetName(i), i);
							SendClientMessage(playerid, GREEN, string);
       						format(string2, sizeof(string2), "%s (%d) da cuop %s tu ban.", GetName(playerid), playerid, formatInt(amount));
	 	    				SendClientMessage(i, RED, string2);
							format(string3, sizeof(string3), "{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Cuop cua si quan thi hanh phap luat, Vi tri: %s.", GetName(playerid), playerid, Zones[current_zone][zone_name]);
      						CopRadio(BLUE, string3);
							break;
						}
						else
						{
	    					SendClientMessage(playerid,RED,"Nguoi choi do qua ngheo ban khong the cuop tu ho");
						    }
		    			}
			 			if(GetTeam{i} == CLASS_CIV || GetTeam{i} == CLASS_MEDIC)
						{
						if (GetPlayerCash(i) > 100)
						{
						    PlayerInfo[playerid][TotalCrimesCommited] += 1;
						    amount = GetPlayerCash(i)/20;
     						//
     						GivePlayerCash(i, - amount);
							GivePlayerCash(playerid, amount);
							PlayerInfo[playerid][TimesPlayerRobbed] += 1;
						    PlayerInfo[playerid][TimesPlayerRobbedCash] += amount;
						    //
						    new string[150], string2[150], string3[150];
						    TimerInfo[playerid][CMD_timer37] = gettime();
						    PlayerInfo[playerid][pCrimes] ++;
							SetPlayerScore(playerid, GetPlayerScore(playerid)+1);
							SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 3);
							new current_zone;
     	 	 				current_zone = Player_Zone[playerid];
					        SendClientMessage(playerid, RED, "Pham toi: Cuop cua dan thuong - Cap do truy na 3 - Da phat lenh.");
							format(string, sizeof(string), "Ban da cuop %s tu %s (%d).", formatInt(amount), GetName(i), i);
							SendClientMessage(playerid, GREEN, string);
       						format(string2, sizeof(string2), "%s (%d) da cuop %s tu ban.", GetName(playerid), playerid, formatInt(amount));
	 	    				SendClientMessage(i, RED, string2);
							format(string3, sizeof(string3), "{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Cuop cua dan thuong, Vi tri: %s.", GetName(playerid), playerid, Zones[current_zone][zone_name]);
      						CopRadio(BLUE, string3);
							break;
						}
						else
						{
						    	SendClientMessage(playerid,RED,"Nguoi choi do qua ngheo ban khong the cuop tu ho");
		    					}
  							}
						}
					}
				}
			}
		}
		}
		if(rangepass == 0)
		{
			SendClientMessage(playerid, RED, "Hien tai khong co nguoi choi nao o gan ban de ban co the cuop.");
		}
		else if(vehiclepass == 0)
		{
			SendClientMessage(playerid, RED, "Ban khong the cuop cua nguoi choi khi ho dang o tren xe.");
		}
		else if(dutypass == 0)
		{
			SendClientMessage(playerid, RED, "Ban khong the cuop admin on-duty.");
		}
		else if(hdutypass == 0)
		{
			SendClientMessage(playerid, RED, "Ban khong the cuop admin on-duty.");
		}
		else if(eventpass == 0)
		{
			SendClientMessage(playerid, RED, "Ban khong the cuop nguoi choi trong su kien.");
		}
		return true;
}

