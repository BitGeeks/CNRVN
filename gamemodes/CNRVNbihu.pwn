		SendClientMessage(playerid,COLOR_ADMIN,"Usage: /aka [Ten tai khoan/ID]");
		return 1;
		}

		if(!IsPlayerConnected(TargetID) || TargetID == INVALID_PLAYER_ID)
	    {
	    SendClientMessage(playerid, RED, "Nguoi choi do khong ket noi.");
	    return 1;
	    }

		if(IsPlayerNPC(TargetID))
	    {
    	SendClientMessage(playerid,RED,"You Cannot Check Aka For Server [BOTS].");
    	return 1;
    	}

 	    GetPlayerIp(TargetID,pIP,50);

        // Active Textdraw Box System
   		ActiveBox[playerid]=1;
  		// END
		
		// Showing Textdraws
		new name[50];
		format(name, sizeof(name), "%s (%d)'s AKA", GetName(TargetID), TargetID);
		TextDrawSetString(Textdrawpaka2, name);
		
		new info[500];
		format(info, sizeof(info), "IP: ~p~%s~n~~n~~w~Names: ~p~%s", pIP, dini_Get("Aka.txt",pIP));
