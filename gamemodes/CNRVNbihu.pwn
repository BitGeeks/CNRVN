public SkydiveInteriorChanger(playerid)
{
	if(!IsPlayerConnected(playerid))
	{
	    KillTimer(skytimer{playerid});
	}
	if(!IsPlayerInRangeOfPoint(playerid, 100.0, 315.745086, 984.969299, 1958.919067))
	{
	    SetPlayerInterior(playerid, 0);
	    KillTimer(skytimer{playerid});
	}
	return 1;
}

public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	// Anti C-Bug System
	if(newkeys & KEY_FIRE && oldkeys & KEY_CROUCH && IsCbugWeapon(playerid))
    {
        CallLocalFunction("OnPlayerMakeCBug", "i", playerid);
    }
	// END
	
	// Textdraws Hidding
	if( ( ( ( newkeys & ( KEY_FIRE) ) == ( KEY_FIRE ) && ( ( oldkeys & ( KEY_FIRE ) ) != ( KEY_FIRE ) ) ) ) )
 	{
	   	// Active Textdraw Box System
 	    if(ActiveBox[playerid] == 1)
        {
            HideDraw(playerid);
 	    	HideTdMenuForPlayer(playerid,CurrentPlayerMenu[playerid]);
            ActiveBox[playerid]=0;
	    	SendClientMessage(playerid,RED,"Da huy lua chon!");
 		}
 		else
 		{
   			HideDraw(playerid);
 	    	HideTdMenuForPlayer(playerid,CurrentPlayerMenu[playerid]);
  		}
    }
    // END
	
	// Cash Delivery Mission System
	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
	{
	    if(cashdelivery[playerid] == 1)
		{
		    cashdelivery[playerid] = 0;
	    	DisablePlayerRaceCheckpoint(playerid);
	    	SendClientMessage(playerid,RED,"Nhiem vu van chuyen that bai, ban da roi khoi chiec xe cua ban!");
	    	// Showing Textdraw
	  		new usemetext[50];
	    	format(usemetext, sizeof(usemetext), "~r~NHIEM VU VAN CHUYEN THAT BAI~n~DA ROI KHOI PHUONG TIEN!", 6000, 3);
		    TextDrawSetString(Textdrawuseme0[playerid], usemetext);
		    TextDrawShowForPlayer(playerid, Textdrawuseme0[playerid]);
		    SetTimerEx("EndAntiSpawnKill", 6000, false, "i", playerid);
		    // END
		}
		if(cashdelivery[playerid] == 2)
		{
		    cashdelivery[playerid] = 0;
	    	DisablePlayerRaceCheckpoint(playerid);
	    	SendClientMessage(playerid,RED,"Nhiem vu van chuyen that bai, ban da roi khoi chiec xe cua ban!");
	    	// Showing Textdraw
	  		new usemetext[50];
	    	format(usemetext, sizeof(usemetext), "~r~NHIEM VU VAN CHUYEN THAT BAI~n~DA ROI KHOI PHUONG TIEN!", 6000, 3);
		    TextDrawSetString(Textdrawuseme0[playerid], usemetext);
		    TextDrawShowForPlayer(playerid, Textdrawuseme0[playerid]);
		    SetTimerEx("EndAntiSpawnKill", 6000, false, "i", playerid);
		    // END
		}
		if(cashdelivery[playerid] == 3)
		{
		    cashdelivery[playerid] = 0;
	    	DisablePlayerRaceCheckpoint(playerid);
	    	SendClientMessage(playerid,RED,"Nhiem vu van chuyen that bai, ban da roi khoi chiec xe cua ban!");
