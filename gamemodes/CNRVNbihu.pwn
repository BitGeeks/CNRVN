// Silent Kick Command
CMD:skick(playerid, params[])
{
    new targetid;
    if(PlayerInfo[playerid][aLevel] < 3) return SendClientMessage(playerid,RED,"Khong tim thay lenh: De xem danh sach lenh day du, nhap /lenh.");
    if(sscanf(params, "us[24]", targetid)) return SendClientMessage(playerid, COLOR_ADMIN, "Usage: /skick [ten tai khoan/id]");
	if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(IsPlayerNPC(targetid)) return SendClientMessage(playerid, RED, "Ban khong the thuc hien lenh nay voi [BOTS] may chu");
    {
        format(fstr, sizeof(fstr), "**SERVER KICK: %s (%d) Da bi kick ra khoi may chu - Phat hien gian lan.", GetName(targetid), targetid);
        SendClientMessageToAll(COLOR_ADMIN, fstr);
        // Sending Message To Admins
    	new astring[250];
		format(astring, sizeof(astring), "{00FF22}%s (%d) {FFFFFF}Da bi kick ra khoi may chu - {F81414}Admin Silent Kick.", GetName(playerid), playerid);
		SendAdminMessage(-1, astring);
  		// END
        // Kick System
		new fstringz[250];
		format(fstringz,sizeof(fstringz),"Ban da bi kick ra khoi may chu - Phat hien gian lan.");
     	// Showing Textdraws
		new usemetext[250];
		format(usemetext, sizeof(usemetext), "~r~BAN DA BI ~p~KICK ~r~RA KHOI~n~~r~MAY CHU VI KHONG DUOC PHEP GIAN LAN.");
		TextDrawSetString(Textdrawuseme0[targetid], usemetext);
		TextDrawShowForPlayer(targetid, Textdrawuseme0[targetid]);
		SetTimerEx("EndAntiSpawnKill", 25000, false, "i", targetid);
		// END
     	SetPlayerInterior(targetid, 3);
		SetPlayerPos(targetid, 193.5904,175.7956,1003.0234);
		SetPlayerFacingAngle(targetid,357.7934);
		SetCameraBehindPlayer(targetid);
     	SendClientMessage(targetid,COLOR_ADMIN,fstringz);
     	SetTimerEx("KickPlayer",100,false,"i",targetid);
     	// END
    }
    return true;
}

