CMD:escape(playerid, params[])
{
    if(GetTeam{playerid} != CLASS_CIV) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
    if(IsJailed{playerid} == false) return SendClientMessage(playerid, RED, "Ban khong o trong nha tu.");
    if(CanESC{playerid} == false) return SendClientMessage(playerid, RED, "Vui long cho truoc khi tron thoat lan nua.");
	new escrand = random(3);
	switch(escrand)
	{
		case 0:
		{
		    // Anti Teleport System
			AntiCheatProtection[playerid] = 1;
			SetTimerEx("AntiTeleportTimer",4000,false,"i",playerid);
			// END
			PlayerInfo[playerid][TotalCrimesCommited] += 1;
        	SetPlayerHealth(playerid, 100.0);
    		SetPlayerInterior(playerid, 0);
			SetPlayerVirtualWorld(playerid,0);
			SetPlayerPos(playerid, 2286.9150,2428.6892,10.8203);
			SetPlayerFacingAngle(playerid, 175.3000);
			SetCameraBehindPlayer(playerid);
        	SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 6);
        	PlayerInfo[playerid][pCrimes] ++;
        	format(fstr, sizeof(fstr),"{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Vuot nguc, Vi tri: Roca Escalante.", GetName(playerid), playerid);
        	CopRadio(BLUE, fstr);
			format(fstr2, sizeof(fstr2), "{00FF22}%s (%d) {FFFFFF}da {00FF22}thanh cong {FFFFFF}trong viec vuoc nguc khoi {FFE600}trai giam.", GetName(playerid), playerid);
       		SendClientMessageToAll(RED, fstr2);
        	SendClientMessage(playerid, RED, "Pham toi: Vuot nguc - Cap do truy na 6 - Da phat lenh.");
			IsJailed{playerid} = false;
			KillTimer(JailTimer[playerid]);
			JailTime[playerid] = 0;
			PlayerInfo[playerid][jTime] = 0;
			if(IsPlayerConnected(playerid) && IsLoggedIn{playerid} == 1)
			{
			    SaveUserStats(playerid);
			}
        }
        case 1..2:
		{
	    	CanESC{playerid} = false;
        	SetTimerEx("WaitToTryEscape", 60000, false, "d", playerid);
        	format(fstr, sizeof(fstr),"{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: Co gang vuot nguc, Vi tri: Roca Escalante.", GetName(playerid), playerid);
        	CopRadio(BLUE, fstr);
        	SendClientMessage(playerid, YELLOW, "Ban da that bai trong viec vuot nguc.");
		}
	}
    return true;
}

CMD:kidnap(playerid, params[])
{
   	new targetid;
   	new current_zone;
   	current_zone = Player_Zone[targetid];
   	new vehicle;
   	vehicle = GetPlayerVehicleID(playerid);
   	if(sscanf(params, "u", targetid)) return SendClientMessage(playerid, RED, "Usage: /kidnap [ten tai khoan/id]");
	if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
   	if(GetTeam{playerid} != CLASS_CIV) return SendClientMessage(playerid, RED, "Tac vu khong cho phep ban thuc hien vi ky nang hien tai cua ban.");
   	if(GetTeam{targetid} == CLASS_MEDIC) return SendClientMessage(playerid, RED, "Ban khong the bat coc bac si.");
   	if(targetid == playerid) return SendClientMessage(playerid, RED, "Ban khong the bat coc chinh minh.");
   	if(!ProxDetectorS(5.0, playerid, targetid)) return SendClientMessage(playerid, RED, "Nguoi choi do khong o gan ban.");
   	if(!IsPlayerInAnyVehicle(targetid) && !IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "Ca hai can phai o tren xe.");
   	if(HasBeenTied{targetid} == true) return SendClientMessage(playerid, RED, "Nguoi choi do da bi bat coc.");
   	if(!IsPlayerInVehicle(targetid, vehicle)) return SendClientMessage(playerid, RED, "Ca hai phai cung o tren mot chiec xe.");
   	if(GetPlayerState(targetid) == PLAYER_STATE_DRIVER) return SendClientMessage(playerid, RED, "Nguoi choi do phai ngoi o ghe hanh khach.");
	if(IsOnduty{targetid} == 1) return SendClientMessage(playerid, RED, "Ban khong the bat coc admin on-duty.");
	if(IsAtEvent{targetid} == 1) return SendClientMessage(playerid, RED, "Ban khong the bat coc nguoi choi dang trong su kien.");
	if(IsAtEvent{targetid} == 0)
	{
	    PlayerInfo[playerid][TotalCrimesCommited] += 1;
		TogglePlayerControllable(targetid, false);
      	HasBeenTied{targetid} = true;
       	PlayerInfo[playerid][pCrimes] ++;
       	SetPlayerWantedLevel(playerid, GetPlayerWantedLevel(playerid) + 5);
		format(fstr, sizeof(fstr), "Ban da bi bat coc boi %s (%d).", GetName(playerid), playerid);
       	SendClientMessage(targetid, RED, fstr);
       	GameTextForPlayer(targetid, "~r~Da bat coc", 3000, 3);
       	GameTextForPlayer(playerid, "~g~Nguoi choi bi bat coc", 3000, 3);
       	SendClientMessage(playerid, GREEN, "Ban da bat coc thanh cong nguoi choi.");
       	SendClientMessage(playerid, RED, "Pham toi: Bat coc nguoi dan - Cap do truy na 5 - Da phat lenh.");
       	format(fstr, sizeof(fstr), "{FF0000}BAO CAO TOI PHAM: {0099CC}Nghi pham: %s (%d) Toi danh: da bat coc %s (%d), Vi tri: %s.", GetName(playerid), playerid, GetName(targetid), targetid, Zones[current_zone][zone_name]);
       	CopRadio(BLUE, fstr);
	}
	else
	{
	    SendClientMessage(playerid, RED, "Ban khong the bat coc nguoi choi dang trong su kien.");
	}
	return true;
}

