// Fine Command
CMD:fine(playerid, params[])
{
    new giveplayerid, amount;
    if(PlayerInfo[playerid][aLevel] < 2) return SendClientMessage(playerid,RED,"Khong tim thay lenh: De xem danh sach lenh day du, nhap /lenh.");
    if(sscanf(params, "ud", giveplayerid, amount)) return SendClientMessage(playerid, COLOR_ADMIN, "Usage: /fine [ten tai khoan/id] [amount]");
    if(!IsPlayerConnected(giveplayerid)) return SendClientMessage(playerid, RED, "Khong tim thay nguoi choi. Vui long nhap ID/Ten tai khoan ro rang hon.");
    if(IsPlayerNPC(giveplayerid)) return SendClientMessage(playerid, RED, "Ban khong the thuc hien lenh nay voi [BOTS] may chu");
    {
        new newtext[150];
	    format(newtext, sizeof(newtext), "%s", formatInt(amount));
	    TextDrawSetString(Textdrawfine1, newtext);
	    TextDrawShowForPlayer(giveplayerid, Textdrawfine1);
	    TextDrawShowForPlayer(giveplayerid, Textdrawfine0);
	    SetTimerEx("EndAntiSpawnKill", 10000, false, "i", giveplayerid);
        GivePlayerCash(giveplayerid, -amount);
        format(fstr, sizeof(fstr), "**ADMIN SERVER: da phat ban %s.", formatInt(amount));
        SendClientMessage(giveplayerid, COLOR_ADMIN, fstr);
        format(fstr, sizeof(fstr), "Ban da phat %s (%d) %s.", GetName(giveplayerid), giveplayerid, formatInt(amount));
        SendClientMessage(playerid, COLOR_ADMIN, fstr);
        // Sending Message To Admins
        new astring[100];
        format(astring, sizeof(astring), "ADMIN: %s (%d) : Da su dung lenh /fine.", GetName(playerid), playerid);
        // END
    }
    return true;
}
